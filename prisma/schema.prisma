generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  name     String?
  surname  String?
  nickname String  @unique
  password String
  verified Boolean @default(false)
}

model Multipart {
  id          Int    @id @default(autoincrement())
  image       String
  description String
}
